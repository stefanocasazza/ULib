{ $as_echo "$as_me:${as_lineno-$LINENO}: result: ${T_MD}Checking for python-embedded package specific:${T_ME}" >&5
$as_echo "${T_MD}Checking for python-embedded package specific:${T_ME}" >&6; }



	#
